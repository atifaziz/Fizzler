    <PackageLicenseUrl>http://www.gnu.org/licenses/lgpl.html</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/atifaziz/Fizzler</PackageProjectUrl>
    <RepositoryUrl>https://github.com/atifaziz/Fizzler</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>selectors w3c html css</PackageTags>
    <AssemblyVersion>1.1.21209.0</AssemblyVersion>
    <FileVersion>1.1.21209.1357</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Fizzler\Fizzler.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack" Version="1.6.5" />
  </ItemGroup>
  <ItemGroup>
